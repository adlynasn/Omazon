<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="stackPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.gui.LoginController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="YELLOW" height="299.0" stroke="BLACK" strokeType="INSIDE" width="285.0" />
      <VBox alignment="CENTER" fillWidth="false" prefHeight="600.0" prefWidth="588.0" spacing="10.0">
         <children>
            <ImageView id="logo" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../resources/img/logo_full.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></ImageView>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Yeah" />
            <TextField fx:id="username" alignment="CENTER" prefHeight="25.0" prefWidth="269.0" />
            <PasswordField alignment="CENTER" prefHeight="25.0" prefWidth="269.0" />
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#setText" text="Login" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</StackPane>
